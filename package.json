{
  "name": "hibari",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "James Harris <wopian@wopian.me>",
  "private": true,
  "scripts": {
    "test": "yarn translation",
    "dev": "node build/dev-server.js",
    "build": "node build/build.js",
    "lint": "eslint --ext .js,.vue .",
    "jest": "jest --coverage",
    "translation": "babel-node tools/translation --presets=env",
    "log:major": "changelog -M",
    "log:minor": "changelog -m",
    "log:patch": "changelog -p"
  },
  "dependencies": {
    "buefy": "~0.6.2",
    "client-oauth2": "~4.1.0",
    "kitsu": "~5.0.0",
    "localforage": "~1.5.5",
    "vue": "~2.5.11",
    "vue-i18n": "~7.4.0",
    "vue-instantsearch": "~1.4.0",
    "vue-lory": "~0.0.5",
    "vue-meta": "~1.4.0",
    "vue-router": "~3.0.1",
    "vue-timeago": "~3.4.2",
    "vue-typer": "~1.2.0",
    "vuex": "~3.0.1",
    "vuex-persist": "~1.1.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.10",
    "autoprefixer": "~8.0.0",
    "babel-cli": "~6.26.0",
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.2",
    "babel-plugin-istanbul": "~4.1.5",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-plugin-webpack-chunkname": "~1.0.1",
    "babel-preset-env": "~1.6.1",
    "babel-preset-stage-0": "~6.24.1",
    "babel-register": "~6.26.0",
    "chalk": "~2.3.0",
    "connect-history-api-fallback": "~1.5.0",
    "copy-webpack-plugin": "~4.4.0",
    "css-loader": "~0.28.7",
    "cssnano": "~3.10.0",
    "eslint": "~4.18.0",
    "eslint-config-wopian-vue": "~1.3.0",
    "eslint-friendly-formatter": "~3.0.0",
    "eslint-loader": "~1.9.0",
    "eventsource-polyfill": "~0.9.6",
    "express": "~4.16.2",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~1.1.5",
    "friendly-errors-webpack-plugin": "~1.6.1",
    "generate-changelog": "~1.7.0",
    "glob-all": "~3.1.0",
    "html-webpack-plugin": "~2.30.1",
    "http-proxy-middleware": "~0.17.4",
    "node-sass": "~4.7.2",
    "optimize-css-assets-webpack-plugin": "~3.2.0",
    "postcss-loader": "^2.0.10",
    "postcss-nested": "^3.0.0",
    "progress-bar-webpack-plugin": "~1.11.0",
    "pug": "~2.0.0-rc.4",
    "pug-loader": "~2.3.0",
    "rimraf": "~2.6.2",
    "sass-loader": "~6.0.6",
    "semver": "~5.5.0",
    "shelljs": "~0.8.0",
    "sugarss": "^1.0.1",
    "sw-precache-webpack-plugin": "~0.11.4",
    "tyu": "~1.0.1",
    "uglifyjs-webpack-plugin": "~1.2.0",
    "url-loader": "~0.6.2",
    "vue-loader": "~14.1.0",
    "vue-style-loader": "~4.0.0",
    "vue-template-compiler": "~2.5.11",
    "webpack": "~4.0.0",
    "webpack-bundle-size-analyzer": "~2.7.0",
    "webpack-dev-middleware": "~2.0.1",
    "webpack-hot-middleware": "~2.21.0",
    "webpack-merge": "~4.1.1"
  },
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0",
    "yarn": ">= 1.0.0"
  },
  "browserslist": [
    "last 2 versions",
    "> 0.5%"
  ],
  "renovate": {
    "extends": [
      ":npm",
      ":preserveSemverRanges",
      ":combinePatchMinorReleases",
      ":renovatePrefix",
      ":semanticCommits",
      ":semanticCommitTypeAll(chore)",
      ":rebaseStalePrs",
      ":unpublishSafe",
      ":prNotPending",
      ":automergeDisabled",
      ":maintainLockFilesMonthly",
      ":ignoreUnstable",
      ":updateNotScheduled",
      ":label(dependencies)",
      ":timezone(Europe/London)",
      ":assignee(wopian)",
      "group:babelMonorepo",
      "group:jestMonorepo",
      "group:storybookMonorepo"
    ],
    "prTitle": "update {{depName}} to {{newVersion}}",
    "commitMessage": "update {{depName}} to {{newVersion}}",
    "baseBranch": "new",
    "packageRules": [
      {
        "packagePatterns": [
          "^webpack"
        ],
        "groupName": "webpack"
      },
      {
        "packagePatterns": [
          "^vue"
        ],
        "groupName": "vue"
      },
      {
        "packagePatterns": [
          "^vuex"
        ],
        "groupName": "vuex"
      }
    ]
  }
}
